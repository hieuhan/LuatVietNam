@model VanBanLuatVN.Models.DocViewModel.DocSearchModel
<div class="search-block">
    <div class="rows search">
        <input class="example-search pagetk" style="font-style: normal; color: #222;" type="text" name="fname" placeholder="Quyết định 161/QĐ-TTg của Thủ tướng Chính phủ về việc xuất cấp gạo từ nguồn dự trữ">
        <button class="btn-default btnsearch pagetk">Tìm kiếm</button>
    </div>
    <button class="advanced-search dropbtn page2">Tìm nâng cao</button>
    <!-- Bengin dropdown-search -->
    <div class="dropdown-search fixwg">
        <div id="advanced-search-panel" class="dropdown-content">
            <div class="row-tim-kiem">
                <div class="tile-tim-kiem font16">
                    Tìm trong:
                </div>
                <div class="loctimkiem">
                    <div class="item-checkbox p5">
                        <div class="radio">
                            <input class="none" id="radio-1" name="radio" type="radio">
                            <label for="radio-1" class="radio-label"> Tất cả </label>
                        </div>
                    </div>
                    <div class="item-checkbox p5">
                        <div class="radio">
                            <input class="none" id="radio-2" name="radio" type="radio">
                            <label for="radio-2" class="radio-label"> Tiêu đề</label>
                        </div>
                    </div>
                    <div class="item-checkbox p5">
                        <div class="radio">
                            <input class="none" id="radio-3" name="radio" type="radio">
                            <label for="radio-3" class="radio-label"> Số hiệu văn bản </label>
                        </div>
                    </div>
                    <div class="item-checkbox p5">
                        <div class="pure-checkbox">
                            <input class="none" id="checkbox4" name="checkbox" type="checkbox">
                            <label for="checkbox4">Chính xác cụm từ</label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="cat-box-content">
                <div class="select-nang-cao-page">
                    <div class="text-label">
                        <select class="text-s10">
                            <option value="0">Ngày ban hành</option>
                            <option value="1">Ngày áp dụng</option>
                        </select>
                    </div>
                    <div class="text-calendar">
                        <div class="item-col2tk">
                            <div class="col-md1">
                                Từ ngày:
                            </div>
                            <div class="col-md2">
                                <input class="text-ms2 italic calendar" placeholder="dd/mm/yyyy">
                            </div>
                        </div>
                        <div class="item-col2tk ">
                            <div class="col-md1">
                                Đến ngày:
                            </div>
                            <div class="col-md2">
                                <input class="text-ms2 italic calendar" placeholder="dd/mm/yyyy">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="select-nang-cao-page">
                    <label class="text-label"> Nhóm văn bản:</label>
                    <div class="text-input-right">
                        <select class="text-s11">
                            <option value="0"></option>
                            <option value="1">Nhóm văn bản</option>
                            <option value="2">Lĩnh vực hoạt động B</option>
                            <option value="3">Lĩnh vực hoạt động A</option>
                            <option value="4">Lĩnh vực hoạt động B</option>
                            <option value="5">Lĩnh vực hoạt động A</option>
                        </select>
                    </div>
                </div>

                <div class="select-nang-cao-page">
                    <label class="text-label"> Loại văn bản:</label>
                    <div class="text-input-right">
                        <select class="text-s11">
                            <option value="0"></option>
                            <option value="">Loại văn bản</option>
                            <option value="1">Loại văn bản</option>
                            <option value="2">Loại văn bản</option>
                            <option value="3">Loại văn bản</option>
                            <option value="4">Loại văn bản</option>
                        </select>
                    </div>
                </div>
                <div class="select-nang-cao-page">
                    <label class="text-label"> Tình trạng hiệu lực:</label>
                    <div class="text-input-right">
                        <select class="text-s11">
                            <option value="0"></option>
                            <option value="1">Lĩnh vực hoạt động A</option>
                            <option value="2">Lĩnh vực hoạt động B</option>
                            <option value="3">Lĩnh vực hoạt động A</option>
                            <option value="4">Lĩnh vực hoạt động B</option>
                            <option value="5">Lĩnh vực hoạt động A</option>
                        </select>
                    </div>
                </div>
                <div class="select-nang-cao-page">
                    <label class="text-label"> Lĩnh vực hoạt động</label>
                    <div class="text-input-right">
                        <select class="text-s11">
                            <option value="0"></option>
                            <option value="1">Lĩnh vực hoạt động A</option>
                            <option value="2">Lĩnh vực hoạt động B</option>
                            <option value="3">Lĩnh vực hoạt động A</option>
                            <option value="4">Lĩnh vực hoạt động B</option>
                            <option value="5">Lĩnh vực hoạt động A</option>
                        </select>
                    </div>
                </div>
                <div class="select-nang-cao-page">
                    <label class="text-label">Cơ quan ban hành</label>
                    <div class="text-input-right">
                        <select class="text-s11">
                            <option value="0"></option>
                            <option value="1">Lĩnh vực hoạt động A</option>
                            <option value="2">Lĩnh vực hoạt động B</option>
                            <option value="3">Lĩnh vực hoạt động A</option>
                            <option value="4">Lĩnh vực hoạt động B</option>
                            <option value="5">Lĩnh vực hoạt động A</option>
                        </select>
                    </div>
                </div>
                <div class="select-nang-cao-page" style="margin-bottom: 0;">
                    <label class="text-label">Người ký văn bản:</label>
                    <div class="text-input-right">
                        <input class="text-s11" type="text" name="fname" value="">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>