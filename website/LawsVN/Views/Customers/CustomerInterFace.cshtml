                                    </tr>
                                    <tr class="row-first-news">
                                        <td class="news-center-post">
                                            <a href="#" class="select-customer-fieldsV2" style="font-style: italic; margin-top:0px"><img class="edit2-img" alt="edit2" src="@Url.Content("~/assets/images/edit2.png")"> Cập nhật lĩnh vực</a>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        }
                    </div>


                    @if (Model.lDocsView_UBND.HasValue())
