                <div class="cat-box-content">

                    <div id="tab-item-1" class="click-1">
                        <table class="table table-bordered">
                            <tbody>
                                <tr class="row-first-news" style="background: #fbf3e8;">
                                    <th class="count-123 page hight2" scope="row"></th>
                                    @if (Model.ListFieldsQuanTamTcvn.HasValue())
                                    {
                                        <td class="news-left-post hight2">
                                            @for (int i = 0; i < Model.ListFieldsQuanTamTcvn.Count && i < 6; i++)
                                            {
                                                var field = Model.ListFieldsQuanTamTcvn[i];
                                                <a href="#" title="@field.FieldDesc" data-id="@field.FieldId" class="item-sub-cat fieldIdTCVN">@field.FieldName</a>
                                            }
                                        </td>
                                        if (Model.ListFieldsQuanTamTcvn.Count >= 6)
                                        {
                                            <td class="post-time-right page hight2">
                                                <div class="dropdown55">
                                                    <select id="ddlFieldTCVN" tabindex="1" class="dropdown">
                                                        <option value="0">@Resource.AllFields</option>
                                                        @for (int i = 6; i < Model.ListFieldsQuanTamTcvn.Count; i++)
                                                        {
                                                            var field = Model.ListFieldsQuanTamTcvn[i];
                                                            <option value="@field.FieldId">@field.FieldDesc</option>
                                                        }
                                                    </select>
                                                </div>
                                            </td>
                                        }
                                    }
                                </tr>
                            </tbody>
                        </table>
                    </div>

                    <div id="Content" class="click-1">
                        @if (Model.lDocsView_TCVN.HasValue())
                        {
                            <table id="tblcontent" class="table table-bordered" data-doctype="@Constants.DocTypesTieuChuanVn">
                                <tbody>
                                    @for (int index = 1; index <= Model.lDocsView_TCVN.Count; index++)
                                    {
                                        var docView = Model.lDocsView_TCVN[index - 1];
                                        <tr class="row-first-news @(index % 2 != 0 ? "" : "bgcolor")">
                                            <th class="count-123 page" scope="row">@index.ToString("#00")</th>
                                            <td class="news-left-post">
                                                <a href="@docView.GetDocUrl()" class="post-title lvn">
                                                    @docView.DocName
                                                </a>
                                                <div class="item-meta">
                                                    @{ Html.RenderAction("PartialLinkDocumentAttribute", "Shared", new { model = new PartialLinkDocumentAttributeModel { mDocsView = docView, ClassName = "tag-item page" } });}
                                                </div>
                                            </td>
                                            <td class="post-time-right page">
                                                @{ Html.RenderAction("PartialLinkDocumentAttribute2", "Shared", new { model = new PartialLinkDocumentAttributeModel { mDocsView = docView } });}
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                                    { Html.RenderAction("PartialPaginationAjax2", "Shared", new { model = Model.PartialPaginationAjaxFirst }); }
                        }
                        else
                        {
                            <table class="table table-bordered">
                                <tbody>
                                    <tr class="row-first-news">
                                        <th class="count-123" scope="row"></th>
                                        <td class="news-center-post">
                                            <h3>
                                                <span class="post-title lvn">@Resource.NoMatchingDocumentFound</span>
                                            </h3>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        }

