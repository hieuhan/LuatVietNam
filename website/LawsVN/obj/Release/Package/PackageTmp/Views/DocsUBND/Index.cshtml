@section FooterSection
{
    <script type="text/javascript">
        jQuery(function () {
            $(".datepicker").datepicker({
                dateFormat: 'dd-mm-yy',
                yearRange: "1945:c",
                changeMonth: true,
                changeYear: true
            });
            $('#DateFrom').datepicker({
                numberOfMonths: 1,
                dateFormat: 'dd/mm/yy',
                changeMonth: true,
                changeYear: true,
                yearRange: "1945:c",
                onSelect: function (selected) {
                    $('#DateTo').datepicker("option", "minDate", selected);
                }
            });
            $('#DateTo').datepicker({
                numberOfMonths: 1,
