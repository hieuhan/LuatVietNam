@{
    Layout = "~/Views/Shared/_LayoutMyLuatVNMobile.cshtml";
}
@using LawsVN.App_GlobalResources
@using LawsVN.Library
@using LawsVN.Models
@using ICSoft.CMSLib
@model CusomersInterfaceModel
@section SeoHeader
{
    <h1 style="text-indent: -20000px;position: absolute;">@Html.Raw((string.IsNullOrEmpty(Model.SeoHeader) ? "Tiêu chuẩn Việt Nam" : Model.SeoHeader.ToString()))</h1>
}
@{Html.RenderAction("PartialFullSearchHomeMobile", "Shared");}

<div class="breadcrumb">
    <a href="@(CmsConstants.ROOT_PATH)giao-dien-ca-nhan.html" title="@Resource.HomePage" class="breadcrumb-item">My luatVietnam</a>»
    <span class="breadcrumb-item color1">Tiêu chuẩn Việt Nam</span>
</div>

<section class="box-section">
    <div class="title">
