    @*<div class="title2">
            <h2 class="title-cat2 vien"> @Model.WebsiteTitle</h2>
        </div>*@
     @if (Model.ListFieldsQuanTamTcvn.HasValue())
    {
    <div class="box-seclect" id="tab-item-1">
        <div class="col3-seclect1">
            <select id="ddlFieldTCVN" name="FieldId" class="select-option" style="padding-right: 16px;">
                <option value="0" title="@Resource.AllFields">@Resource.AllFields</option>
                @foreach (var item in Model.ListFieldsQuanTamTcvn)
                {
                    <option value="@item.FieldId" title="@item.FieldName">@item.FieldName</option>
                }
            </select>
        </div>
    </div>
     }
    <!-- End-->
    <!-- content-->
    <div id="Content" class="cat-box-content">
        @if (Model.lDocsView_TCVN.HasValue())
        {
            for (int i = 0; i < Model.lDocsView_TCVN.Count; i++)
            {
                <div class="first-news @(i == Model.lDocsView_TCVN.Count - 1 ? "last" : "dashed")">
                    <h3 class="post-title">
                        <a href="@Model.lDocsView_TCVN[i].GetDocUrl()" title="@Model.lDocsView_TCVN[i].DocName"><span class="stt">(@((i + 1).ToString("#00")))</span>@Model.lDocsView_TCVN[i].DocName</a>
                    </h3>
                    <div class="post-time">
                        @{ Html.RenderAction("PartialLinkDocumentAttribute2Mobile", "Shared", new { model = new PartialLinkDocumentAttributeModel { mDocsView = Model.lDocsView_TCVN[i] } });}
                    </div>
                </div>
            }
            Model.PartialPaginationAjaxFirst.IsMobile = true;
            Html.RenderAction("PartialPaginationAjax4", "Shared", new { model = Model.PartialPaginationAjaxFirst });
        }
        else
        {
            <div class="first-news last">
                <span>@Resource.NoMatchingDocumentFound</span>
            </div>
        }
    </div>
    <!-- End-->
</section>



@if (Model.lDocsView_Newest.HasValue())
{
    <div class="title" style="margin-bottom: 0;">
        <h2 class="title-cat"> Tiêu chuẩn Việt Nam mới cập nhật</h2>
    </div>
    <div class="cat-box-content">
        @foreach (var docView in Model.lDocsView_Newest)
        {
            <div class="first-news dashed">
                <h3 class="post-title"><a href="@docView.DocUrl" title="@docView.DocName">@docView.DocName</a></h3>
                <div class="post-time">
                    @{ Html.RenderAction("PartialLinkDocumentAttribute2Mobile", "Shared", new { model = new PartialLinkDocumentAttributeModel { mDocsView = docView } });}
                </div>
            </div>
        }
    </div>
}