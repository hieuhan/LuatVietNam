@section FooterSection
{
    <script type="text/javascript">
        jQuery(function () {
            $(".datepicker").datepicker({
                dateFormat: 'dd/mm/yy',
                yearRange: "1945:c",
                changeMonth: true,
                changeYear: true
            });
